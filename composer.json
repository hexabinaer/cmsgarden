{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=5.6",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/address": "^1.6",
        "drupal/adminimal_theme": "^1.5",
        "drupal/auto_entitylabel": "^2.1@beta",
        "drupal/bootstrap_barrio": "^4.22",
        "drupal/bootstrap_sass": "^1.9",
        "drupal/cer": "^4.0@alpha",
        "drupal/chosen": "^2.7",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.6.0",
        "drupal/datetime_extras": "^1.0@alpha",
        "drupal/diff": "^1.0@RC",
        "drupal/drush_locale_sync": "^2.0",
        "drupal/ds": "^3.3",
        "drupal/entity_browser": "^2.1",
        "drupal/entity_browser_enhanced": "^1.0@RC",
        "drupal/entity_limit": "^2.0@alpha",
        "drupal/entity_usage": "^2.0@alpha",
        "drupal/field_group": "^1.0",
        "drupal/geolocation": "3.0-rc1",
        "drupal/metatag": "^1.8",
        "drupal/paragraphs": "^1.8",
        "drupal/paragraphs_collection": "^1.0@alpha",
        "drupal/paragraphs_jquery_ui_accordion": "^1.3",
        "drupal/pathauto": "^1.4",
        "drupal/redirect": "^1.3",
        "drupal/registration_validation": "^1.2",
        "drupal/svg_image": "^1.9",
        "drupal/swiftmailer": "^1.0@beta",
        "drupal/viewfield": "^3.0@alpha",
        "drupal/views_bulk_edit": "^2.3",
        "drupal/views_bulk_operations": "^3.1",
        "drupal/webform": "^5.2",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.6.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}

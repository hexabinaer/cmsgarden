{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=5.6",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/address": "^1.7",
        "drupal/address_formatter": "^2.0",
        "drupal/allowed_formats": "^1.2",
        "drupal/auto_entitylabel": "^3.0@beta",
        "drupal/cer": "^4.0@alpha",
        "drupal/chosen": "^3.0",
        "drupal/ckeditor_markdown": "^1.2",
        "drupal/coffee": "^1.0@beta",
        "drupal/config_split": "^1.4",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.6.0",
        "drupal/date_range_formatter": "^3.1",
        "drupal/datetime_extras": "^1.0@alpha",
        "drupal/diff": "^1.0@RC",
        "drupal/drush_locale_sync": "^2.0",
        "drupal/entity_browser": "^2.4",
        "drupal/entity_browser_enhanced": "^1.0",
        "drupal/entity_embed": "^1.0",
        "drupal/entity_limit": "^2.0@alpha",
        "drupal/entity_reference_revisions": "^1.7",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/environment_indicator": "^4.0",
        "drupal/field_group": "^3.0",
        "drupal/field_permissions": "^1.0@RC",
        "drupal/flag": "^4.0@beta",
        "drupal/geolocation": "~3.0",
        "drupal/git_deploy": "^2.3",
        "drupal/hacked": "^2.0@beta",
        "drupal/honeypot": "^1.30",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/layout_builder_at": "^2.11",
        "drupal/layout_builder_modal": "^1.1",
        "drupal/layout_builder_restrictions": "^2.7",
        "drupal/licenses": "^2.0",
        "drupal/media_entity_browser": "~2",
        "drupal/metatag": "^1.11",
        "drupal/paragraphs": "^1.12",
        "drupal/password_policy": "^3.0@alpha",
        "drupal/pathauto": "^1.6",
        "drupal/poll": "^1.3",
        "drupal/prepopulate": "^2.2",
        "drupal/profile": "^1.1",
        "drupal/realname": "^1.0@RC",
        "drupal/redirect": "^1.5",
        "drupal/registration_validation": "^1.2",
        "drupal/replicate_ui": "^1.0",
        "drupal/reroute_email": "^1.2",
        "drupal/role_delegation": "^1.1",
        "drupal/simplify": "^1.1",
        "drupal/social_media_links": "^2.6",
        "drupal/stage_file_proxy": "^1.0",
        "drupal/svg_image": "^1.9",
        "drupal/swiftmailer": "^1.0@beta",
        "drupal/token": "^1.6",
        "drupal/tzfield": "^1.3",
        "drupal/views_add_button": "^1.4",
        "drupal/views_bulk_edit": "^2.4",
        "drupal/views_bulk_operations": "^3.5",
        "drupal/views_conditional": "^1.0",
        "drupal/views_infinite_scroll": "^1.6",
        "drupal/webform": "^5.8",
        "drush/drush": "^9.0.0",
        "harvesthq/chosen": "^1.8",
        "oomphinc/composer-installers-extender": "^2.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "drupal/asset_injector": "^2.6",
        "drupal/devel": "^4.0",
        "drupal/migrate_tools": "^4.5",
        "drupal/upgrade_status": "^2.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/profile": {
                "[#2899744] Make Profiles Translatable": "https://www.drupal.org/files/issues/2020-02-07/make-profiles-translatable-2899744-28.patch"
            },
            "drupal/realname": {
                "[#3070681] Implement visibility options for 'Real name' field label": "https://www.drupal.org/files/issues/2020-01-26/3070681-13.patch"
            }
        },
        "installer-types": ["library"],
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library", "harvesthq/chosen"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"],
            "vendor/{$vendor}/{$name}": ["type:library"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
